				}
				
				if(user != null)
				{
					//sendTheMessage("/MoveToShow/"+user.userId.toString()+"/"+user.emailId+"/"+user.showID);
					_toMovetoShowArray.addItem(user);
					_movingUser = user.userId;
					
					_pleaseWaitAlert = null;
