					PopUpManager.addPopUp(_pleaseWaitAlert,this,true);
					PopUpManager.centerPopUp(_pleaseWaitAlert);
					moveUserstoShow(0)
				}
				if(_toMovetoShowArray.length == 0)
				{
					rearangeScreeningOrder();
					//rearrageScreeningUsers(true);
				}
			}
			
